{
	"name": "copy_data_container_to_sql",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "ParquetSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "ds_generic_parquet",
						"type": "DatasetReference",
						"parameters": {
							"containerName": "bronze",
							"fileName": "person.person.parquet"
						}
					}
				],
				"outputs": [
					{
						"referenceName": "ds_generic_azure_db",
						"type": "DatasetReference"
					}
				]
			}
		],
		"folder": {
			"name": "PIPELINES_SQL"
		},
		"annotations": []
	}
}